# Private key for deployment (DO NOT commit your actual .env file)
PRIVATE_KEY=your_private_key_here

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# RPC URLs for different networks
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR-PROJECT-ID
MAINNET_RPC_URL=https://mainnet.infura.io/v3/YOUR-PROJECT-ID

# Gas reporting (set to true to enable)
REPORT_GAS=false

# Deployment configuration (optional - defaults are in deploy.js)
# QUORUM_BASIS_POINTS=4000  # 40%
# VOTING_PERIOD=50400        # ~7 days
# VOTING_DELAY=7200          # ~1 day
